{
  "name": "origin-web-catalogs",
  "version": "1.0.0",
  "description": "The Openshift Web Catalogs Library",
  "scripts": {
    "clean": "rimraf dist && rimraf docs && rimraf coverage",
    "build": "rimraf dist && webpack --bail --progress --profile",
    "build:run": "npm run build && live-server --port=3001 --open=dist/",
    "server": "webpack-dev-server --history-api-fallback --inline --progress",
    "coverage": "live-server --port=3001 --open=coverage/",
    "test": "karma start",
    "test:coverage": "npm run test && npm run coverage",
    "test:watch": "karma start --auto-watch --no-single-run",
    "docs": "jsdoc src/app -r -d docs",
    "docs:launch": "npm run docs && live-server --port=3002 --open=docs/",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/origin/origin-web-catalogs.git"
  },
  "author": "Jeff Phillips, David Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/origin/origin-web-catalogs/issues"
  },
  "homepage": "https://github.com/origin/origin-web-catalogs",
  "dependencies": {
    "@angular/router": "^0.2.0",
    "angular": "^1.5.0",
    "angular-patternfly": "v4.0.0-alpha.2",
    "angular-sanitize": "^1.5.3",
    "angular-ui-select": "^0.12.100",
    "angular-ui-bootstrap": "2.4.x",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.6.1",
    "paper-css": "^0.1.2",
    "patternfly": "3.18.1",
    "jquery": "~2.1.4"
  },
  "devDependencies": {
    "@types/angular": "^1.5.16",
    "@types/angular-mocks": "^1.5.5",
    "@types/es6-shim": "^0.31.32",
    "@types/jasmine": "^2.5.38",
    "@types/jquery": "^2.0.33",
    "@types/node": "^0.0.2",
    "angular-mocks": "^1.5.0",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "eslint": "^2.8.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-promise": "^1.1.0",
